<br>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">user name</th>
      <th scope="col">email</th>
      <th scope="col">role</th>
      <th scope="col">Admin</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each_with_index do |u,index|%>
    <tr>
      <% if u.role == "User" || u.role == "Creator"%>
        <th scope="row"><%= index+1 %></th>
        <td><%= u.user_name %></td>
        <td><%= u.email %></td>
        <td><%= u.role %></td>
        <td><%= u.admin %></td>
        <td>
          <%= link_to "Edit",edit_user_registration_path(id:u.id), class:"btn btn-success text-light" %>
          <%= link_to "Delete",user_registration_path(id:u.id),method: :delete, class:"btn btn-danger text-light", data: { confirm: 'Are you sure?' } %>  
        </td>
      <% end %>  
    </tr>
    <% end %>
  </tbody>
</table>
